<UserControl x:Class="AssemblyPatcher.Wpf.Views.ClassView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cal="http://www.caliburnproject.org"
             xmlns:designMocks="clr-namespace:AssemblyPatcher.Wpf.DesignMocks"
             mc:Ignorable="d" 
             d:DesignWidth="300"
             x:Name="UC"
             d:DataContext="{d:DesignInstance Type=designMocks:ClassViewModelMock, IsDesignTimeCreatable=True}"
             cal:Bind.AtDesignTime="True"
             IsHitTestVisible="False"
             Focusable="False">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock x:Name="ClassName" />
        </StackPanel>
    </StackPanel>
</UserControl>
